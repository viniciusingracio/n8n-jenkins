dir: /var/bigbluebutton/mconf-data
{% if mconf_data_elements | length > 0 %}
data:
{{ mconf_data_elements | to_nice_yaml }}
{% else %}
mconf_data_elements: []
{% endif %}
s3:
  enabled: {{ mconf_data_s3_enabled }}
  bucket_name: {{ mconf_data_s3_bucket_name }}
  region: {{ mconf_data_s3_region }}
  endpoint: {{ mconf_data_s3_endpoint }}
  key: {{ mconf_data_s3_key }}
  secret: {{ mconf_data_s3_secret }}
default_locale: pt
default_timezone: America/Sao_Paulo
default_timezone_offset: '-0300'

